void printTriangle(int n) {
        // code here
        int x=0;
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=i;j++)
            {
                if((j-i)%2)
                cout<<"0"<<" ";
                else
                cout<<"1"<<" ";
            }
            cout<<endl;
        }
    }
