void printTriangle(int n) {
        // code here
        for(int i=1;i<=n;i++)
        {
            for(int d=i;d<n;d++)
            cout<<" ";
            for(int k=1;k<i;k++)
            cout<<char('A'+k-1);
            cout<<char('A'+i-1);
            for(int j=i-1;j>=1;j--)
            cout<<char('A'+j-1);
            cout<<endl;
        }
    }
