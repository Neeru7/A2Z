void printTriangle(int n) {
        // code here
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=i;j++)
            cout<<j<<" ";
            for(int x=1;x<=2*(n-i);x++)
            cout<<" "<<" ";
            for(int k=i;k>=1;k--)
            cout<<k<<" ";
            cout<<endl;
            
        }
    }
